{
    "name": "@eighty4/dank",
    "version": "0.0.2",
    "type": "module",
    "license": "MIT",
    "author": "Adam McKee Bennett <adam.be.g84d@gmail.com>",
    "packageManager": "pnpm@10.17.1",
    "description": "Multi-page development system for CDN-deployed websites",
    "keywords": [
        "frontend",
        "bundler",
        "esbuild",
        "dank"
    ],
    "bin": "./lib_js/bin.js",
    "exports": {
        ".": {
            "node": "./lib_js/dank.js",
            "types": "./lib_types/dank.d.ts",
            "import": "./lib/dank.ts"
        }
    },
    "scripts": {
        "build": "tsc && tsc -p tsconfig.client.json && tsc -p tsconfig.exports.json",
        "fmt": "prettier --write .",
        "fmtcheck": "prettier --check .",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "esbuild": "^0.25.10",
        "mime": "^4.1.0",
        "parse5": "^8.0.0"
    },
    "devDependencies": {
        "@types/node": "^24.5.2",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2"
    },
    "files": [
        "client/esbuild.js",
        "lib/*.ts",
        "lib_js/*.js",
        "lib_types/*.d.ts"
    ]
}

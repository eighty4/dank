{
    "name": "@eighty4/dank",
    "version": "0.0.1-2",
    "type": "module",
    "packageManager": "pnpm@10.17.0",
    "bin": "./lib_js/bin.js",
    "exports": {
        ".": {
            "node": "./lib_js/dank.js",
            "types": "./lib_types/dank.d.ts",
            "import": "./lib/dank.ts"
        }
    },
    "scripts": {
        "build": "tsc && tsc -p tsconfig.client.json && tsc -p tsconfig.exports.json",
        "fmt": "prettier --write .",
        "fmtcheck": "prettier --check .",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "esbuild": "^0.25.10",
        "parse5": "^8.0.0"
    },
    "devDependencies": {
        "@types/node": "^24.5.2",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2"
    },
    "files": [
        "client/esbuild.js",
        "lib/*.ts",
        "lib_js/*.js",
        "lib_types/*.d.ts"
    ]
}
